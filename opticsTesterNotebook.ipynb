    "    \n",
    "    @classmethod\n",
    "    def beam_source(cls, x0, y0, x1, y1, numberOfRays, startLambda=1, endLambda=1):\n",
    "        if x1 - x0 == 0 and y1 - y0 == 0 :\n",
    "            raise(Exception(\"beam width must be non-zero\"))\n",
    "        rayAngle = np.arctan2((x1-x0),-(y1-y0))\n",
